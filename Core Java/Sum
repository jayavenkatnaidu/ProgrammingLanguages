import java.lang.*;

//sum of two numbers
public class Sum {

    public static void main(String args[]){
        int x,y;
        x=10;
        y=20;

        //calculate sum and store result into z
        int z = x+y;
        //display result
        System.out.println("Sum of Two numbers: "+z);
        //pre increment and post increment
        System.out.println(z);
        System.out.println(++z);
        System.out.println(z);
        System.out.println(z++);
        System.out.println(z);


    }
}
